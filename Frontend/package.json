{
  "name": "ncrsapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --open",
    "publish": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --aot",
    "publishNoHashProd": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production --aot --output-hashing=none",
    "publishNoHashPreProd": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration preProduction --aot --output-hashing=none",
    "publishNoHashTest": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration testing --aot --output-hashing=none",
    "watch": "ng serve --host 0.0.0.0 --port 4300 --open --watch --configuration development",
    "watchTest": "ng serve --host 0.0.0.0 --port 4300 --open --watch --configuration testing",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint --fix"
  },
  "private": true,
